{
	"compilerOptions": {
		"alwaysStrict": true,
		"baseUrl": ".",
		"incremental": true,
		"isolatedModules": true,
		"moduleResolution": "node",
		"noImplicitThis": true,
		"outDir": "output",
		"paths": {"/*": ["*"]},
		"removeComments": true,
		"strictBindCallApply": true,
		"strictFunctionTypes": true,
		"target": "esnext",
		"typeRoots": ["node_modules/@types", "typings"],
	},
	"exclude": ["node_modules", "templates"],
}
